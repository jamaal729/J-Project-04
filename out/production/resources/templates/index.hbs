{{#partial "content"}}

    <header>
        <div class="container">
            <div class="site-header">
                <a class="logo" href="/">SparkBlog</a>
                <a class="new-entry button button-round" href="/new"><i class="material-icons">create</i></a>
            </div>
        </div>
    </header>
    <section>
        <div class="container">
            <div class="entry-list">
                {{#each entries}}
                    <article>
                        <h2><a href="entry/{{slug}}">{{title}}</a></h2>
                        <time>{{date}}</time>
                    </article>
                {{/each}}
            </div>
        </div>
    </section>
    <footer>


{{/partial}}

{{> base.hbs}}

